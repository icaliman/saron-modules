.terminal-target
  width 100%
  height 100%
  overflow hidden

.terminal
  float left
  border #000000 solid 5px
  font-family "DejaVu Sans Mono", "Liberation Mono", monospace
  font-size 11px
  color #f0f0f0
  background #000000
  width calc(100% - 0px)
  height calc(100% - 0px)

.terminal-cursor 
  color #000000
  background #f0f0f0