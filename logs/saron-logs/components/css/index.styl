.logs-target
  color: #0C0
  padding: 5px
  width: 100%
  height: 100%
  position absolute

  .highlight
    background #338ffe
    color #fff

  .log-list
    height 100%
    overflow: auto


  .log-filter
    background #428BCA
    position: absolute
    top 5px
    right 5px
    padding 6px
    display none
    color white
    border: 1px solid rgba(0, 0, 0, 0);
    border-radius: 4px;
    label
      margin 0
    input
      height 100%
      margin-left 4px
      border none
      padding 2px 4px

  p
    font-size: 12px
    line-height: 16px
    font-weight: 400
    font-family: 'Inconsolata',Courier,sans-serif

